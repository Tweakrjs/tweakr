{
  "name": "tweakr",
  "version": "1.1.0",
  "description": "A modern, modular utility library for JavaScript and TypeScript with functional, async, and immutable helpers.",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/tweakr.esm.js",
      "require": "./dist/tweakr.cjs.js"
    }
  },
  "sideEffects": false,
  "keywords": [
    "utils",
    "utility",
    "typescript",
    "modular",
    "async",
    "functional",
    "immutable",
    "helpers"
  ],
  "license": "MIT",
  "author": "Coxlee Anthony Hubilla <https://github.com/coxxanthony>",
  "main": "dist/tweakr.cjs.js",
  "module": "dist/tweakr.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "docs": "typedoc",
    "deploy-docs": "gh-pages -d docs",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "prepare": "husky",
    "release": "standard-version",
    "commit": "cz",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "test:array": "vitest run test/array",
    "test:async": "vitest run test/async",
    "test:browser": "vitest run test/browser",
    "test:function": "vitest run test/function",
    "test:number": "vitest run test/number",
    "test:object": "vitest run test/object",
    "test:string": "vitest run test/string",
    "test:util": "vitest run test/util"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Tweakrjs/tweakr.git"
  },
  "bugs": {
    "url": "https://github.com/Tweakrjs/tweakr/issues"
  },
  "homepage": "https://tweakrjs.github.io/tweakr/",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^3.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "rollup": "^4.52.4",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typedoc-github-theme": "^0.3.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.ts": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
